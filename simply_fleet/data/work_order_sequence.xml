<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences -->
        <record id="seq_vehicle_work_order" model="ir.sequence">
            <field name="name">Vehicle Work Order</field>
            <field name="code">simply.vehicle.work.order</field>
            <field name="prefix">WO/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Default Stock Locations -->
        <record id="location_work_order" model="stock.location">
            <field name="name">Work Order Location</field>
            <field name="usage">internal</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
