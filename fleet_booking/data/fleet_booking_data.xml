<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for Fleet Booking -->
        <record id="seq_fleet_booking" model="ir.sequence">
            <field name="name">Fleet Booking</field>
            <field name="code">fleet.booking</field>
            <field name="prefix">ORD</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Sequences for Cargo Manifest -->
        <record id="seq_fleet_cargo_manifest" model="ir.sequence">
            <field name="name">Fleet Cargo Manifest</field>
            <field name="code">fleet.cargo.manifest</field>
            <field name="prefix">CM</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>